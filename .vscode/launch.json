{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug PitchforkTests",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/tests/PitchforkTests",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": false
        }
      ]
    },
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
  ]
}